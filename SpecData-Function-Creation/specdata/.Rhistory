pollutantmean <- function(directory, pollutant, id = 1:332) {
all_files <- list.files(directory,full.names = TRUE)
dat <- data.frame()
for (i in id){
dat <- rbind(dat, read.csv(all_files[i]))
}
dat_subset <- dat[which(dat[,"ID"]%in% id),]
mean(dat[, pollutant],na.rm = TRUE)
}
